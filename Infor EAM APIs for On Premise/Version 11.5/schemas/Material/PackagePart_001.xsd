<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/PackagePart_001"
    xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.datastream.net/MP_entities/PackagePart_001" elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd" />
    <!--==================== Begining of Package Part ==================== -->
    <xs:element name="PackagePart">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:PACKAGEPARTID" />
                <xs:element ref="dsf:PARTID" minOccurs="0" />
                <xs:element ref="dsf:AVAILABLEQUANTITY" minOccurs="0" />
