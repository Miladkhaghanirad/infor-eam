<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/ContractorSignOff_001"
    xmlns="http://schemas.datastream.net/MP_entities/ContractorSignOff_001"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    elementFormDefault="qualified">
    <!-- =========== Imported namespaces ================== -->
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--======== Beginning of ContractorSignOff_001 ===============================-->
    <xs:element name="ContractorSignOff">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:PERMITTOWORKCODE"/>
                <xs:element ref="dsf:CONTRACTORSIGNOFFNAME"/>
                <xs:element ref="dsf:DATESIGNED"/>
                <xs:element ref="dsf:SIGNATURETYPE" minOccurs="0"/>
                <xs:element ref="dsf:REMARK" minOccurs="0"/>
                <xs:element ref="dsf:COMPANYNAME" minOccurs="0"/>
                <xs:element ref="dsf:CONTRACTORRESPONSIBILITY" minOccurs="0"/>
                <xs:element ref="dsf:MOBILEDATECREATED" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="recordid" type="xs:long"></xs:attribute>
        </xs:complexType>
    </xs:element>
</xs:schema>