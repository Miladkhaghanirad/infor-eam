<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/EquipmentLinearCondition_001" 
    xmlns="http://schemas.datastream.net/MP_entities/EquipmentLinearCondition_001" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:dsf="http://schemas.datastream.net/MP_fields" elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--===============================================================================-->
    <xs:element name="EquipmentLinearCondition">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:LINEAREQUIPMENTCONDITIONID" minOccurs="0"/>
                <xs:element ref="dsf:HEADEREQUIPMENTID"/>
                <xs:element ref="dsf:LINEARDIRECTIONID" minOccurs="0"/>
                <xs:element ref="dsf:CONDITIONSCORE" minOccurs="0"/>
                <xs:element ref="dsf:CONDITIONINDEX" minOccurs="0"/>
                <xs:element ref="dsf:RISKPRIORITYNUMBER" minOccurs="0"/>
                <xs:element ref="dsf:RISKPRIORITYINDEX" minOccurs="0"/>
                <xs:element ref="dsf:CRITICALITYSCORE" minOccurs="0"/>
                <xs:element ref="dsf:CRITICALITYID" minOccurs="0"/>
                <xs:element ref="dsf:FROMPOINT"/>
                <xs:element ref="dsf:FROMREFERENCEID" minOccurs="0"/>
                <xs:element ref="dsf:FROMOFFSET" minOccurs="0"/>
                <xs:element ref="dsf:FROMOFFSETPERCENTAGE" minOccurs="0"/>
